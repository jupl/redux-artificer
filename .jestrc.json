{
  "coverageDirectory": "<rootDir>/.coverage",
  "coveragePathIgnorePatterns": [
    "/node_modules/",
    "<rootDir>/.jest/"
  ],
  "moduleFileExtensions": [
    "js",
    "json",
    "ts",
    "tsx"
  ],
  "testRegex": "(/__tests__/.*|\\.(test|spec))\\.tsx?$",
  "transform": {
    ".tsx?": "ts-jest"
  }
}
